{
  "name": "atom-focus-mode",
  "main": "./lib/main",
  "version": "0.13.0",
  "description": "Fades editor content and highlights only the lines you are working on.",
  "activationCommands": {
    "atom-workspace": [
      "atom-focus-mode:toggle-cursor-focus",
      "atom-focus-mode:toggle-single-line-focus",
      "atom-focus-mode:toggle-shadow-focus",
      "atom-focus-mode:toggle-scope-focus"
    ]
  },
  "repository": "https://github.com/davidleghorn/atom-focus-mode",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0"
  },
  "keywords": [
    "distraction free",
    "focus",
    "writing",
    "editing",
    "productivity"
  ]
}
